// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Spot {
  id               Int      @id @default(autoincrement())
  name             String
  description      String?
  latitude         Decimal
  longitude        Decimal
  windStrengthUnit Decimal
  windStrenghtMax  Decimal
  windStrengthMin  Decimal
  windAzimuthUnit  Decimal
  windAzimuthMax   Decimal
  windAzimuthMin   Decimal
  createdAt        DateTime @default(now())
  updatedAt        DateTime @updatedAt
}
